{
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "crlf",
	"overrides": [
		{
			"files": ["*.json"],
			"options": {
				"parser": "json"
			}
		},
		{
			"files": ["*.ts"],
			"options": {
				"parser": "typescript"
			}
		}
	],
	"printWidth": 80,
	"proseWrap": "preserve",
	"semi": true,
	"singleQuote": false,
	"trailingComma": "es5",
	"useTabs": true
}
